{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "labels": ["dependencies"],
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchPackageNames": ["bootstrap"],
      "allowedVersions": "<=4.6.1",
      "description": "vue-bootstrap doesn't support boostrap 5",
      "enabled": false
    }
  ]
}
